<?xml version="1.0" ?>
<robot name="camel_dong_dog">

  <material name="black">
    <color rgba="0.0 0.0 0.0 0.9"/>
  </material>

  <material name="white">
    <color rgba="0.0 0.0 0.0 1"/>
  </material>

  <material name="gray1">
    <color rgba="0.75 0.75 0.75 1"/>
  </material>

  <material name="gray2">
    <color rgba="0.5 0.5 0.5 0.9"/>
  </material>

  <material name="gray3">
    <color rgba="0.25 0.25 0.25 0.7"/>
  </material>

  <link name="world"/>

  <joint name="fixed_world" type="fixed">
    <parent link="world"/>
    <child link="base"/>
  </joint>

  <link name="base">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 1.25"/>
      <geometry>
        <box size="0.25 0.25 2.5"/>
      </geometry>
      <material name="black"/>
    </visual>

    <inertial>
      <mass value="10"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="base" type="prismatic">
    <origin rpy="0 0 0" xyz="-0.25 0 -0.3"/>
    <parent link="base"/>
    <child link="shoulder"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="shoulder">
    <visual>
      <origin rpy="0 0 0" xyz="-0.825 -0.25 -1"/>
      <geometry>
        <mesh filename="shoulder.stl" />
      </geometry>
      <material name="gray1"/>
    </visual>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>

  <link name="thigh">
    <visual>0.43 -0.3 -0.3
      <origin rpy="0 0 0" xyz="-0.175 -0.915 -1.2"/>
      <geometry>
        <mesh filename="thigh.stl"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>

  <link name="calf">
    <visual>
      <origin rpy="0 0 0" xyz="-0.075 -0.285 -0.765"/>
      <geometry>
        <mesh filename="calf.stl"/>
      </geometry>
      <material name="gray3"/>
    </visual>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>

  <joint name="J1" type="revolute">
    <origin rpy="0 0 0" xyz="-0.25 0.475 0.2"/>
    <parent link="shoulder"/>
    <child link="thigh"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="J2" type="revolute">
    <origin rpy="0 0 0" xyz="-0.075 -0.725 -0.725"/>
    <parent link="thigh"/>
    <child link="calf"/>
    <axis xyz="1 0 0"/>
  </joint>
</robot>