<?xml version="1.0" ?>
<robot name="camel_dong_dog">

  <material name="black">
    <color rgba="0.0 0.0 0.0 0.7"/>
  </material>

  <material name="gray1">
    <color rgba="0.75 0.75 0.75 0.7"/>
  </material>

  <material name="gray2">
    <color rgba="0.5 0.5 0.5 0.7"/>
  </material>

  <material name="gray3">
    <color rgba="0.25 0.25 0.25 0.7"/>
  </material>

  <link name="world"/>

  <joint name="fixed_world" type="fixed">
    <parent link="world"/>
    <child link="base"/>
  </joint>


  <link name="base">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 2.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="base" type="prismatic">
    <origin rpy="0 0 0" xyz="0 -0.025 0.0"/>
    <parent link="base"/>
    <child link="shoulder"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="shoulder">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="shoulder.stl"/>
      </geometry>
      <material name="gray1"/>
    </visual>
  </link>

  <joint name="J1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 1"/>
    <parent link="shoulder"/>
    <child link="thigh"/>
    <axis xyz="0 -1 0"/>
  </joint>

  <link name="thigh">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="thigh.stl"/>
      </geometry>
      <material name="gray2"/>
    </visual>
  </link>


</robot>

<joint name="J2" type="revolute">
<origin rpy="0 0 0" xyz="0 0 0"/>
<parent link="thigh"/>
<child link="calf1"/>
</joint>

<link name="calf1">
<visual>
  <origin rpy="0 0 0" xyz="0 0 0"/>
  <geometry>
    <mesh filename="calf1.stl"/>
  </geometry>
  <material name="gray3"/>
</visual>
</link>

<joint name="J3" type="revolute">
<origin rpy="0 0 0" xyz="0 0 0"/>
<parent link="calf2"/>
<child link="calf3"/>
</joint>

<link name="calf2">
<visual>
  <origin rpy="0 0 0" xyz="0 0 0"/>
  <geometry>
    <mesh filename="calf2.stl"/>
  </geometry>
  <material name="gray2"/>
</visual>
</link>

<joint name="J4" type="revolute">
<origin rpy="0 0 0" xyz="0 0 0"/>
<parent link="shoulder"/>
<child link="calf3"/>
<axis xyz="0 0 0"/>
</joint>

<link name="calf3">
<visual>
  <origin rpy="0 0 0" xyz="0 0 0"/>
  <geometry>
    <mesh filename="calf3.stl"/>
  </geometry>
  <material name="gray3"/>
</visual>
</link>

<joint name="J5" type="revolute">
<origin rpy="0 0 0" xyz="0 0 0"/>
<parent link="calf3"/>
<child link="calf2"/>
<axis xyz="0 0 0"/>
</joint>



<link name="base">
<visual>
  <origin rpy="0 0 0" xyz="0 0 1"/>
  <geometry>
    <box size="1 1 1"/>
  </geometry>
  <material name="black"/>
</visual>
</link>

<joint name="base" type="prismatic">
<origin rpy="0 0 0" xyz="0 0 0"/>
<parent link="base"/>
<child link="shoulder"/>
<axis xyz="0 0 0"/>
</joint>

